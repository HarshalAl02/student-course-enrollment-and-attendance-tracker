<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enrollment_Date_Not_Future</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  NOT(ISBLANK(Enrollment_Date__c)),
  Enrollment_Date__c &gt; TODAY()
)</errorConditionFormula>
    <errorDisplayField>Enrollment_Date__c</errorDisplayField>
    <errorMessage>Date cannot be in the future</errorMessage>
</ValidationRule>
